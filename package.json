{
  "name": "snipsnap-vscode",
  "displayName": "Snipsnap - Ultimate Snippet Handler",
  "description": "Code faster with snippets for your favourite libraries",
  "version": "0.1.9",
  "publisher": "snipsnapdev",
  "icon": "snipsnap-logo-stub.png",
  "homepage": "https://marketplace.visualstudio.com/items?itemName=snipsnapdev.snipsnap-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/snipsnapdev/snipsnap-vscode.git"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.snipsnap_activate",
    "workspaceContains:package.json"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.snipsnap_activate",
        "title": "Fetch the snippets",
        "category": "Snipsnap"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cleanup": "npm run lint;prettier --write **/**.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "eslint": "^6.6.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1"
  }
}
